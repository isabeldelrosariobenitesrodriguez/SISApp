        <TextView
            android:id="@+id/textView11"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:text="Centro de Salud." />
      </LinearLayout>

      <LinearLayout
          android:layout_width="match_parent"
          android:layout_height="48dp"
          android:gravity="center"
